{
    "builds": [
      {
        "src": "front-end/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      },
      {
        "src": "back-end/api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/back-end/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/front-end/$1"
      }
    ]

  }